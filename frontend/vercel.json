{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/frontend/browser"
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/polyfills-(.*).js", "dest": "/polyfills-$1.js" },
    { "src": "/main-(.*).js", "dest": "/main-$1.js" },
    { "src": "/styles-(.*).css", "dest": "/styles-$1.css" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
